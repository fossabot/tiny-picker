{
    "env": {
        "browser": true
    },
    "rules": {
        "indent": ["error", 4],
        "no-plusplus": "off",
        "prefer-rest-params": "off",
        "prefer-destructuring": "off",
        "func-names": "off",
        "no-param-reassign": "off"
    },
    "extends": "eslint-config-airbnb-es5"
}